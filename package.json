{
  "name": "rasa-webchat",
  "version": "0.3.0@7",
  "description": "Chat web widget for React apps and Rasa Core chatbots",
  "main": "lib/chat.js",
  "repository": "git@https://github.com/botfront/rasa-webchat.git",
  "author": "",
  "license": "Apache-2.0",
  "scripts": {
    "local": "cross-env ENVIRONMENT=local webpack-dev-server --config webpack.dev.js",
    "dev": "cross-env ENVIRONMENT=development webpack-dev-server --config webpack.dev.js",
    "stage": "cross-env ENVIRONMENT=staging webpack-dev-server --config webpack.dev.js",
    "prod": "cross-env ENVIRONMENT=production webpack-dev-server --config webpack.dev.js",
    "build": "",
    "build-dev": "cross-env ENVIRONMENT=development webpack --config webpack.prod.js",
    "build-stage": "cross-env ENVIRONMENT=staging webpack --config webpack.prod.js",
    "build-prod": "cross-env ENVIRONMENT=production webpack --config webpack.prod.js",
    "build-off": "cross-env ENVIRONMENT=stub webpack --config webpack.stub.js",
    "test": "jest",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "prepare": "npm run build",
    "release": "standard-version",
    "knip": "knip"
  },
  "keywords": [
    "react",
    "chat",
    "widget",
    "javascript"
  ],
  "dependencies": {
    "@popperjs/core": "^2.4.0",
    "@stomp/stompjs": "^5.4.2",
    "dotenv": "^17.2.0",
    "html-webpack-plugin": "^5.5.3",
    "immutable": "^3.8.2",
    "object-hash": "^1.1.5",
    "prop-types": "^15.7.2",
    "react-immutable-proptypes": "^2.2.0",
    "react-markdown": "^10.1.0",
    "react-popper": "^2.3.0",
    "react-redux": "^9.2.0",
    "react-slick": "^0.30.0",
    "react-textarea-autosize": "^8.5.0",
    "redux": "^5.0.0",
    "remark-gfm": "^4.0.1",
    "slick-carousel": "^1.8.1",
    "socket.io-client": "^4.8.1",
    "sockjs-client": "^1.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/eslint-parser": "^7.23.10",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.1",
    "@babel/preset-typescript": "^7.28.5",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.10",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "enzyme": "^3.11.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^2.7.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^25.5.4",
    "knip": "^5.82.1",
    "prettier": "^1.18.2",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "redux-mock-store": "^1.5.4",
    "sass": "^1.71.1",
    "sass-loader": "^13.3.2",
    "standard-version": "^9.0.0",
    "string-replace-loader": "^3.1.1",
    "style-loader": "^3.3.3",
    "terser-webpack-plugin": "^5.3.9",
    "typescript": "^5.9.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "peerDependencies": {
    "react": ">=19.0.0",
    "react-dom": ">=19.0.0"
  },
  "jest": {
    "verbose": true,
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/mocks/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/mocks/styleMock.js"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/test-setup.js"
  }
}